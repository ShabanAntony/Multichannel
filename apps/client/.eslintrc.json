{
  "extends": [
    "plugin:@nx/react",
    "../../.eslintrc.json",
    "plugin:effector/recommended",
    "plugin:effector/scope"
  ],
  "plugins": ["effector"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
